        </activity>
        <activity android:name=".gesture.GestureTestActivity" />
        <activity android:name=".viewpager.ScreenSlidePageActivity" />
        <activity android:name=".bpbackgroudservice.IntentServiceTestActivity" />

        <service
            android:name=".bpbackgroudservice.MyIntentService"
            android:exported="false" />

        <activity android:name=".lifecycle.LifeCycleActivity" />
        <activity android:name=".rankview.WebViewActivity"></activity>
    </application>

</manifest>